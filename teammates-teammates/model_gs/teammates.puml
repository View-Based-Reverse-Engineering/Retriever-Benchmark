    [Browser]
    [Client]
}

package Backend {
    [Logic]
    [Storage]
    [User Interface]
}

package Tests {
    [End-to-End Tests]
    [Test Driver]
}

package "Google App Engine" {
    [Datastore]
}

[Browser] ..> [User Interface] : requires
